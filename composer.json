{
  "name": "thereddot/swiftmailer-image-embed",
  "type": "library",
  "description": "Swiftmailer plugin to automatically embed images into message",
  "keywords": [ "swiftmailer", "image" ],
  "homepage": "https://github.com/TheRedDot/swiftmailer-image-embed",
  "license": "MIT",
  "authors": [
    { "name": "Nicolas Hart" },
    { "name": "Floran Brutel" }
  ],
  "require": {
    "php": ">=7.1",
    "swiftmailer/swiftmailer": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "suggest": {
    "openbuildings/swiftmailer-css-inliner": "Inline css in your Swiftmailer messages"
  },
  "autoload": {
    "psr-4": { "Hexanet\\Swiftmailer\\": "src" }
  },
  "autoload-dev": {
  },
  "replace": {
    "hexanet/swiftmailer-image-embed": "self.version"
  },
  "config": {
      "platform": {"php": "7.1"}
    }
}
